{
  "manifest_version": 2,

  "name": "tatkal",
  "description": "This extension allows the user to change the background color of the current page.",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent":false
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}